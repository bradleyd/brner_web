<h1>Email Messages for <%= @user.email_address%></h1>
<table >
  <tr>
    <th width="250">To</th>
    <th width="250">From</th>
    <th width="250">Subject</th>
    <th width="250">Sent AT</th>
  </tr>

  <% @emails.each do |email| %>
    <tr>
      <td><%= email.recipients %></td>
      <td><%= link_to email.sender, messages_show_path(id: email.id) %></td>
      <td><%= email.subject %></td>
      <td><%= email.created_at %></td>
    </tr>
  <% end %>
</table>
<div class="pagination-centered">
  <!-- <#%= paginate @emails %> -->
</div>
